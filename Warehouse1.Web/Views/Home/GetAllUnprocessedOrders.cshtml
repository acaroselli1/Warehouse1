
@{
    ViewData["Title"] = "Open Orders";
}

<h2>Open Orders</h2>

<ul>
    @foreach (var order in ViewBag.AllUnprocessedOrders)
    {
    <li style="margin:20px">
        @order.Id
        @order.CreatedAt
        @if (order.ClosedAt == null){
        <button class="btn btn-danger btn-xs">Process Order</button>
        }
    </li>
    }
</ul>